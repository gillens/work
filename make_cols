#!/bin/bash

file="outtable.txt"

echo -n "" > ip_list.txt
echo -n "" > name_list.txt


for ip in $(cut -d ',' -f1 $file); do
  ip="${ip//\"}"
  echo $ip >> ip_list.txt
done

readarray -t nameCol < <(cut -d, -f2 $file)
for name in "${nameCol[@]}"; do
  name="${name//\"}"
  char=" "
  if ! [[ $name =~ $char ]]; then
    name="${name%%.*}"
  fi
  echo $name >> name_list.txt
done
